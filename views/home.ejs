<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home - URL Shortener</title>
    <link rel="icon" type="image/png" href="data:image/jpeg;base64,">
    <%- include('./partials/style') %>
</head>
<body>

<h1>URL Shortener</h1>

<div class="container">
    <% if (locals.shorturl) { %>
        <p class="message">URL Generated: <a href=" https://57a1-152-58-196-51.ngrok-free.app/<%= shorturl %>"> https://57a1-152-58-196-51.ngrok-free.app/<%= shorturl %></a></p>
    <% } %>

    <form action="/url" method="post">
        <label for="url">Enter your URL</label>
        <input type="text" id="url" name="url" required placeholder="https://example.com">
        <button type="submit">Generate</button>
    </form>

    <% if (locals.error) { %>
        <p class="message"><%= error %></p>
    <% } %>

    <% if (locals.urls) { %>
        <table>
            <thead>
                <tr>
                    <th>S.No</th>
                    <th>Short URL</th>
                    <th>Redirect URL</th>
                </tr>
            </thead>
            <tbody>
                <% urls.forEach((url, index) => { %>
                <tr>
                    <td><%= index + 1 %></td>
                    <td><a href="<%= url.shorturl %>"><%= url.shorturl %></a></td>
                    <td><a href="<%= url.redirectUrl %>"><%= url.redirectUrl %></a></td>
                </tr>
                <% }) %>
            </tbody>
        </table>
    <% } %>
</div>

<div class="container">
    <% if (locals.users) { %>
        <table>
            <thead>
                <tr>
                    <th>S.No</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Role</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach((user, index) => { %>
                <tr>
                    <td><%= index + 1 %></td>
                    <td><%= user.name %></td>
                    <td><%= user.email %></td>
                    <td><%= user.role %></td>
                </tr>
                <% }) %>
            </tbody>
        </table>
    <% } %>
</div>

</body>
</html>
